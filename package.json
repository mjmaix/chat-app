{
  "name": "chat-app",
  "version": "1.1.1",
  "private": true,
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "import-sort-style-module",
      "options": {}
    }
  },
  "scripts": {
    "start": "./scripts/killpackager.sh && node node_modules/react-native/local-cli/cli.js start -- --reset-cache",
    "test": "jest",
    "postinstall": "patch-package",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "postversion": "./node_modules/.bin/react-native-version",
    "debugger": "./scripts/run-debugger.sh",
    "ios": "npm run debugger && react-native run-ios",
    "ios:se": "npm run ios -- --simulator 'iPhone SE'",
    "ios:tablet": "npm run ios -- --simulator 'iPad Air'",
    "ios:release": "react-native run-ios --configuration Release --scheme Chat-release",
    "android": "npm run debugger && react-native run-android",
    "android:release": "react-native run-android --variant release",
    "cleanup:all": "npm run cleanup:android && npm run cleanup:react && npm run cleanup:metrobundler && npm run cleanup:hastecache && npm run cleanup:ios",
    "cleanup:react": "./scripts/killpackager.sh && watchman watch-del-all && rm -rf node_modules/ && rm -rf $TMPDIR/react-* ~/.npm/ ~./rncache",
    "postcleanup:react": "echo 'run `npm install` or `yarn install`'",
    "cleanup:ios": "rm -rf ~/Library/Developer/Xcode/DerivedData/* ios/build ios/Pods",
    "cleanup:android": "./scripts/cleanandroid.sh",
    "cleanup:metrobundler": "rm -rf /tmp/metro-bundler-cache-*",
    "cleanup:hastecache": "rm -rf /tmp/haste-map-react-native-packager-*",
    "xpostinstall": "./scripts/postinstall.sh && npm run ios:pod:install",
    "xios:pod:install": "cd ios && pod install",
    "studio": "open -a /Applications/Android\\ Studio.app ./android/",
    "xcode": "open ios/Chat.xcodeproj",
    "generator-rn-toolbox": "./scripts/generator-rn-toolbox.sh",
    "patch-package:update": "npx patch-package react-native-elements metro aws-amplify-react-native",
    "ios:simulator:mountimages": "xcrun simctl addmedia booted ./assets/test/avatar1.jpeg ./assets/test/avatar2.jpeg ./assets/test/avatar3.jpeg ./assets/test/avatar4.png",
    "fastlane:deploy:ios": "cd ios && fastlane beta",
    "fastlane:deploy:android": "cd android && fastlane deploy",
    "fastlane:deploy:all": "npm run fastlane:deploy:ios && npm run fastlane:deploy:android"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.3.3",
    "@react-native-community/netinfo": "3.2.1",
    "@types/uuid": "3.4.4",
    "aws-amplify": "1.1.28",
    "aws-amplify-react-native": "2.1.12",
    "aws-appsync": "1.8.0",
    "base-64": "0.1.0",
    "buffer": "5.2.1",
    "eventemitter3": "3.1.2",
    "formik": "1.5.7",
    "graphql-tag": "2.10.1",
    "hex-to-rgb": "1.0.1",
    "jest-styled-components": "6.3.1",
    "lodash": "4.17.11",
    "mime": "2.4.3",
    "react": "16.8.6",
    "react-apollo": "2.5.6",
    "react-native": "0.59.9",
    "react-native-animatable": "1.3.2",
    "react-native-elements": "1.1.0",
    "react-native-gesture-handler": "1.3.0",
    "react-native-gifted-chat": "0.9.3",
    "react-native-image-picker": "0.28.1",
    "react-native-keyboard-aware-scroll-view": "0.8.0",
    "react-native-qrcode-svg": "5.1.2",
    "react-native-svg": "9.4.0",
    "react-native-vector-icons": "6.5.0",
    "react-navigation": "3.11.0",
    "rn-fetch-blob": "0.10.15",
    "styled-components": "4.3.1",
    "utility-types": "3.7.0",
    "uuid": "3.3.2",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/runtime": "7.4.5",
    "@types/base-64": "0.1.3",
    "@types/jest": "24.0.13",
    "@types/lodash": "4.14.134",
    "@types/mime": "2.0.1",
    "@types/react": "16.8.19",
    "@types/react-native": "0.57.60",
    "@types/react-native-elements": "0.18.0",
    "@types/react-test-renderer": "16.8.1",
    "@types/styled-components": "4.1.16",
    "@types/yup": "0.26.14",
    "babel-jest": "24.8.0",
    "babel-plugin-styled-components": "1.10.0",
    "jest": "24.8.0",
    "metro-react-native-babel-preset": "0.54.1",
    "patch-package": "6.1.2",
    "react-native-testing-library": "1.8.0",
    "react-native-version": "3.1.0",
    "react-test-renderer": "16.8.6",
    "ts-jest": "24.0.2",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.0.0",
    "tslint-react-hooks": "2.1.0",
    "typescript": "3.5.1",
    "typescript-styled-plugin": "0.14.0"
  }
}
