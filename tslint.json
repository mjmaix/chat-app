{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-react-hooks"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "jsx-double"],
    "react-hooks-nesting": "error" // tslint-react-hooks
  }
}
